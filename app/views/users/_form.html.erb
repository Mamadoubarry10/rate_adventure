<%= form_for @user do |f| %>
    <%= f.label :name %> 
    <%= f.text_field :name %><br>
    <%= f.label :email %>
    <%= f.text_field :email %><br>
    <%= f.label :age %>
    <%= f.number_field :age %><br>
     <%= f.label :image %>
    <%= f.text_field :image %><br>
    <%= f.label :password %>
    <%= f.password_field :password %><br>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %><br>
    <%= f.submit "Register" %>
<% end %>